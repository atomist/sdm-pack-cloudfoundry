{
  "name": "@atomist/sdm-pack-cloudfoundry",
  "version": "2.0.0",
  "description": "Atomist SDM extension pack for Cloud Foundry",
  "author": {
    "name": "Atomist",
    "email": "support@atomist.com",
    "url": "https://atomist.com/"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/atomist/sdm-pack-cloudfoundry#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/sdm-pack-cloudfoundry.git"
  },
  "keywords": [
    "atomist",
    "automation",
    "sdm",
    "extension",
    "pack",
    "cloud",
    "cloudfoundry"
  ],
  "bugs": {
    "url": "https://github.com/atomist/sdm-pack-cloudfoundry/issues"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@atomist/microgrammar": "^1.2.0",
    "@atomist/slack-messages": "^1.1.1",
    "@types/lodash": "^4.14.116",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10"
  },
  "peerDependencies": {
    "@atomist/automation-client": ">=1.3.0",
    "@atomist/sdm": ">=1.4.0",
    "@atomist/sdm-core": ">=1.5.0",
    "@atomist/sdm-pack-build": ">=1.0.4"
  },
  "devDependencies": {
    "@atomist/automation-client": "^1.6.0",
    "@atomist/sdm": "^1.5.1",
    "@atomist/sdm-core": "^1.5.2",
    "@atomist/sdm-pack-build": "^1.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.7",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.0",
    "rimraf": "^2.6.3",
    "supervisor": "^0.12.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js{,.map}}\" \"{lib,test}/**/*.{d.ts,js{,.map}}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "compile": "run-s gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "fmt": "tsfmt --replace",
    "gql:gen": "atm-gql-gen",
    "lint": "tslint --format verbose --project . --exclude \"node_modules/**\" --exclude \"**/*.d.ts\" \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  }
}
